{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/free-game-finder/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.(ico|js|css|woff2|png|svg|jpg|jpeg|gif)",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}